model
{
	for(i in 1:94)
	{
		Pid[i] <- 1
		Zid[i] <- 1 -( (1/c)*pow(D[i],-1*beta)/I[i])
		Muid[i] <- (0.9*0.5*((Zid[i]/abs(Zid[i])) + 1) + 0.1)*exp(-1*0.5*abs(Zid[i]))
		Pid[i] ~ dbern(Muid[i])
	}
	c ~ dunif(0.1,100)
	beta ~ dunif(0.1,2)
}

# Data
list(D = c(24, 24, 24, 24, 24, 24, 48, 48, 48, 48, 48, 48, 48, 48, 72, 72, 72, 72, 72, 72, 72, 96, 96, 96, 96, 96, 96, 96, 96, 96, 96, 96, 96, 120, 120, 120, 120, 120, 120, 120, 120, 120, 120, 120, 120, 144, 144, 144, 144, 144, 144, 144, 144, 144, 144, 168, 168, 168, 168, 168, 192, 192, 192, 192, 192, 216, 216, 216, 216, 216, 216, 240, 240, 264, 264, 264, 264, 264, 288, 288, 312, 336, 336, 360, 360, 360, 384, 384, 408, 408, 408, 456, 504, 504), I = c(124, 18.6, 19, 102, 70, 35.2, 61, 42, 39, 30.5, 124, 84, 85, 146.6, 282, 118, 81, 32, 240, 141.6, 79.8, 185, 191.2, 186, 73.8, 164.2, 47.3, 280.5, 31.9, 39.6, 279.4, 82, 90.2, 54.4, 588, 391, 137, 73, 209.4, 50.9, 210, 44, 133, 270.2, 333.4, 163.8, 107.2, 623, 85.2, 250, 112.6, 72, 159.1, 257, 343, 468.4, 162, 330, 271.4, 80.6, 538, 151.1, 70.2, 50.2, 84.6, 83.9, 553, 399.6, 86.6, 324.4, 140.8, 516.8, 143.2, 742.1, 274.7, 137.3, 209.4, 179.8, 660, 320.9, 232.3, 158.6, 597, 380.5, 185.4, 249, 248.1, 259.2, 202.2, 308, 559.6, 229, 398.8, 592.2))

#initial values
list(c = 1, beta = 1)